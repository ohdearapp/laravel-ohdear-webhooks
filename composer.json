{
    "name": "ohdearapp/laravel-ohdear-webhooks",
    "description": "Handle Oh Dear webhook calls in a Laravel app",
    "keywords": [
        "ohdear",
        "uptime",
        "monitoring",
        "laravel-ohdear-webhooks"
    ],
    "homepage": "https://github.com/ohdear/laravel-ohdear-webhooks",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "support@ohdear.app",
            "homepage": "https://ohdear.app",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"

    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpunit/phpunit": "^11.3.4",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "OhDear\\LaravelWebhooks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OhDear\\LaravelWebhooks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OhDear\\LaravelWebhooks\\OhDearWebhooksServiceProvider"
            ]
        }
    }
}
