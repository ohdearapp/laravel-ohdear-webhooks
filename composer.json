{
    "name": "ohdearapp/laravel-ohdear-webhooks",
    "description": "Handle Oh Dear webhook calls in a Laravel app",
    "keywords": [
        "ohdear",
        "uptime",
        "monitoring",
        "laravel-ohdear-webhooks"
    ],
    "homepage": "https://github.com/ohdear/laravel-ohdear-webhooks",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "support@ohdear.app",
            "homepage": "https://ohdear.app",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"

    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "orchestra/testbench": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "OhDear\\LaravelWebhooks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OhDear\\LaravelWebhooks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OhDear\\LaravelWebhooks\\OhDearWebhooksServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-v2": "2.0.x-dev"
        }
    }
}
